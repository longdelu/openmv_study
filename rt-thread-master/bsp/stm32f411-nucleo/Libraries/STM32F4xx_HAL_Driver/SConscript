import rtconfig
Import('RTT_ROOT')
from building import *

# get current directory
cwd = GetCurrentDir()

# The set of source files associated with this SConscript file.

src = Glob('Src/*.c')

path = [cwd + '/Inc']

CPPDEFINES = ['USE_HAL_DRIVER']
group = DefineGroup('STM32F4xx_HAL_Driver', src, depend = [''], CPPPATH = path, CPPDEFINES = CPPDEFINES)

Return('group')

